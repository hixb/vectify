{
  "name": "vectify",
  "version": "2.0.1",
  "packageManager": "pnpm@9.15.9",
  "description": "A powerful command-line tool to generate React, Vue, and Svelte icon components from SVG files",
  "author": "Xiaobing Zhu <hellozxb252@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/hixb/vectify#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hixb/vectify.git"
  },
  "bugs": "https://github.com/hixb/vectify/issues",
  "keywords": [
    "svg",
    "icon",
    "icons",
    "cli",
    "react",
    "vue",
    "svelte",
    "component",
    "generator",
    "typescript",
    "vectify",
    "vector"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "vectify": "./dist/cli.js"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cheerio": "^1.1.2",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "domhandler": "^5.0.3",
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.12",
    "jiti": "^2.6.1",
    "ora": "^7.0.1",
    "svgo": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.0.0",
    "@changesets/cli": "^2.29.8",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.10.5",
    "eslint": "^9.39.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
